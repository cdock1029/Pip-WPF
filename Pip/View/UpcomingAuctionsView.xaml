<UserControl x:Class="Pip.UI.View.UpcomingAuctionsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModel="clr-namespace:Pip.UI.ViewModel"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance Type=viewModel:UpcomingAuctionsViewModel}">
    <Grid>
        <ScrollViewer Grid.Row="0">
            <ItemsControl ItemsSource="{Binding Treasuries}" />
        </ScrollViewer>
    </Grid>
</UserControl>
